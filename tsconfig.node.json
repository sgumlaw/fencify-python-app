{
  "include": ["vite.config.*", "vitest.config.*", "playwright.config.*", "eslint.config.*"],
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["node"],
    "noEmit": true,
    "skipLibCheck": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "strict": true,
    "forceConsistentCasingInFileNames": true
  }
}
